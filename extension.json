{
	"name": "Less",
	"author": [
		"Cqm",
		"TehKittyCat"
	],
	"url": "https://runescape.wiki",
	"descriptionmsg": "less-desc",
	"version": "0.1",
	"license-name": "GPL-2.0+",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.45.0"
	},
	"ContentHandlers": {
		"less": {
			"class": "MediaWiki\\Extension\\Less\\ContentHandler"
		}
	},
	"HookHandlers": {
		"codeeditor": {
			"class": "MediaWiki\\Extension\\Less\\CodeEditorHooks",
			"services": [
				"MainConfig"
			]
		},
		"codemirror": {
			"class": "MediaWiki\\Extension\\Less\\CodeMirrorHooks",
			"services": [
				"MainConfig"
			]
		},
		"main": {
			"class": "MediaWiki\\Extension\\Less\\Hooks"
		}
	},
	"Hooks": {
		"CodeEditorGetPageLanguage": "codeeditor",
		"CodeMirrorGetMode": "codemirror",
		"ContentHandlerDefaultModelFor": "main"
	},
	"MessagesDirs": {
		"less": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Less\\": "includes/"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "less/modules"
	},
	"ResourceModules": {
		"ext.less.messages": {
			"messages": [
				"less-update-css",
				"less-dialog-title",
				"less-dialog-close",
				"less-dialog-debug-enabled",
				"less-dialog-getting-source",
				"less-dialog-getting-mixins",
				"less-dialog-attempt-parse",
				"less-dialog-import-success",
				"less-dialog-import-error",
				"less-dialog-formatting-css",
				"less-dialog-getting-header",
				"less-dialog-edit-success",
				"less-dialog-edit-summary",
				"less-dialog-internal-error",
				"less-dialog-page-not-found",
				"less-dialog-check-imports",
				"less-dialog-parse-error-file",
				"less-dialog-unknown-error",
				"less-dialog-error-persist"
			]
		}
	},
	"attributes": {
		"SyntaxHighlight": {
			"Models": {
				"less": "less"
			}
		}
	},
	"config": {
		"LessUseCodeEditor": {
			"value": true,
			"description": "Use CodeEditor for editing Less pages."
		},
		"LessUseCodeMirror": {
			"value": false,
			"description": "Use CodeMirror for editing Less pages."
		}
	},
	"manifest_version": 2
}
